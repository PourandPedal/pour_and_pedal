!!!
%html
  %head
    = render 'layouts/typekit'
    %title [TITLE]
    = stylesheet_link_tag    'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    = yield :head
    /[if IE]
      = javascript_include_tag 'html5'
  %body
    #page
      = render 'layouts/header'
      #main
        .container
          = render 'flashes'
          = yield
      = render 'layouts/footer'
